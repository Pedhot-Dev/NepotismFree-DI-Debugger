{
    "name": "pedhot-dev/nepotismfree-di-debugger",
    "description": "CLI Debugger for NepotismFree-DI",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../nepotismfree-di",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.0",
        "pedhot-dev/nepotismfree-di": "*"
    },
    "autoload": {
        "psr-4": {
            "PedhotDev\\NepotismFree\\Debugger\\": "src/"
        }
    },
    "bin": [
        "bin/di"
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}